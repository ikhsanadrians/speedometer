{
  "expo": {
    "name": "speedometer",
    "slug": "speedometer",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/logospedo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/centerlogo.png",
      "resizeMode": "contain",
      "height" : 10,
      "width" : 10,           
      "backgroundColor": "#161D3F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi":true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0ef631f2-8825-4651-95bc-a88c505a95f9"
      }
    },
    "owner": "adrianikhsandev"
  }
}
